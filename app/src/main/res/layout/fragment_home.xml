<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.HomeFragment">

    <ProgressBar
        android:id="@+id/loading"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:layout_marginTop="64dp"
        android:layout_marginBottom="64dp"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.3"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/background_gray"
        android:layout_width="424dp"
        android:layout_height="770dp"
        android:background="#D1D1D1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.401"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.292"
        app:srcCompat="@android:drawable/screen_background_light_transparent" />

    <com.baidu.mapapi.map.MapView
        android:id="@+id/bmapView"
        android:clickable="true"
        android:layout_width="400dp"
        android:layout_height="310dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.52"
        app:layout_constraintStart_toStartOf="@+id/background_gray"
        app:layout_constraintTop_toTopOf="@+id/background_gray"
        app:layout_constraintVertical_bias="0.17000002" />

    <ImageButton
        android:id="@+id/BackButton"
        android:layout_width="48dp"
        android:layout_height="48dp"
        tools:ignore="SpeakableTextPresentCheck"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/back_icon" />

    <ImageView
        android:id="@+id/Logo1"
        android:layout_width="57dp"
        android:layout_height="51dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.02"
        app:layout_constraintStart_toEndOf="@+id/BackButton"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/logo" />

    <TextView
        android:id="@+id/ProdName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Citizen Express"
        tools:ignore="SpeakableTextPresentCheck"
        app:layout_constraintBottom_toBottomOf="@+id/Logo1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/Logo1"
        app:layout_constraintTop_toTopOf="@+id/Logo1"
        app:layout_constraintVertical_bias="0.47000003" />

    <ImageView
        android:id="@+id/Background2"
        android:layout_width="400dp"
        android:layout_height="294dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/background_gray"
        app:layout_constraintVertical_bias="0.81"
        app:srcCompat="@drawable/shape" />

    <Button
        android:id="@+id/Send"
        android:layout_width="75dp"
        android:layout_height="48dp"
        android:background="@drawable/shape"
        android:backgroundTint="#5E75B9"
        android:text="Send"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.03"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/Get"
        android:layout_width="75dp"
        android:layout_height="48dp"
        android:background="@drawable/shape"
        android:backgroundTint="#5E75B8"
        android:text="Get"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.28"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/radioImg1"
        android:layout_width="37dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.49"
        app:srcCompat="@drawable/radio_button_img" />

    <ImageView
        android:id="@+id/radioImg"
        android:layout_width="37dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.29000002"
        app:srcCompat="@drawable/radio_button_img" />

    <ImageView
        android:id="@+id/radioImg2"
        android:layout_width="37dp"
        android:layout_height="31dp"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.027"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.7"
        app:srcCompat="@drawable/radio_button_img" />

    <View

        android:id="@+id/line"
        android:layout_width="320dp"
        android:layout_height="1dp"
        android:background="#D1D1D1"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.39" />

    <View

        android:id="@+id/line2"
        android:layout_width="320dp"
        android:layout_height="1dp"
        android:background="#D1D1D1"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.6" />

    <View

        android:id="@+id/line4"
        android:layout_width="320dp"
        android:layout_height="1dp"
        android:background="#D1D1D1"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.76"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.8" />

    <Button
        android:id="@+id/Submit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#36509E"
        android:text="Submit"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/Pickup"
        android:layout_width="230dp"
        android:layout_height="48dp"
        android:background="@color/white"
        android:hint="Pick-up address"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.25" />

    <EditText
        android:id="@+id/Consignee"
        android:layout_width="230dp"
        android:layout_height="48dp"
        android:background="@color/white"
        android:hint="Consignee address"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.51" />

    <Button
        android:id="@+id/Item"
        android:layout_width="230dp"
        android:layout_height="48dp"
        android:background="@color/white"
        android:text="Item"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="@+id/Background2"
        app:layout_constraintEnd_toEndOf="@+id/Background2"
        app:layout_constraintHorizontal_bias="0.67"
        app:layout_constraintStart_toStartOf="@+id/Background2"
        app:layout_constraintTop_toTopOf="@+id/Background2"
        app:layout_constraintVertical_bias="0.76" />

    <Button
        android:id="@+id/BtnLogout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#36509D"
        android:text="backToWelcome"
        android:textColor="#FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.933"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/background_gray"
        app:layout_constraintVertical_bias="0.03" />


</androidx.constraintlayout.widget.ConstraintLayout>