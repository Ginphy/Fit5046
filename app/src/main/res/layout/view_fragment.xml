<?xml version="1.0" encoding="utf-8"?>
<androidx.drawerlayout.widget.DrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.home.CourierHomeActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/background_gray"
            android:layout_width="424dp"
            android:layout_height="770dp"
            android:background="#BCBCBC"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.401"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.292"
            android:src="@android:drawable/screen_background_light_transparent" />


        <ImageView
            android:id="@+id/Background"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/background"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />



        <ImageView
            android:id="@+id/Background3"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.861"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toTopOf="@+id/background_gray"
            app:layout_constraintVertical_bias="0.04" />

        <ImageView
            android:id="@+id/background4"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.833"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toTopOf="@+id/background_gray"
            app:layout_constraintVertical_bias="0.146" />

        <TextView
            android:id="@+id/Profile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Profile"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/Background3"
            app:layout_constraintEnd_toEndOf="@+id/Background3"
            app:layout_constraintHorizontal_bias="0.12"
            app:layout_constraintStart_toStartOf="@+id/Background3"
            app:layout_constraintTop_toTopOf="@+id/Background3" />

        <TextView
            android:id="@+id/Nickname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nickname"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background4"
            app:layout_constraintEnd_toEndOf="@+id/background4"
            app:layout_constraintHorizontal_bias="0.05"
            app:layout_constraintStart_toStartOf="@+id/background4"
            app:layout_constraintTop_toBottomOf="@+id/Background3" />

        <ImageView
            android:id="@+id/background6"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toTopOf="@+id/background7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.833"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toBottomOf="@+id/Background5"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/Background5"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.805"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toTopOf="@+id/background_gray"
            app:layout_constraintVertical_bias="0.322" />

        <ImageView
            android:id="@+id/background8"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toTopOf="@+id/background9"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.805"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toBottomOf="@+id/background7"
            app:layout_constraintVertical_bias="0.0" />

        <ImageView
            android:id="@+id/background9"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.805"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toBottomOf="@+id/background7"
            app:layout_constraintVertical_bias="0.292" />

        <ImageView
            android:id="@+id/background7"
            android:layout_width="380dp"
            android:layout_height="72dp"
            android:src="@drawable/shape"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.833"
            app:layout_constraintStart_toStartOf="@+id/background_gray"
            app:layout_constraintTop_toBottomOf="@+id/Background5"
            app:layout_constraintVertical_bias="0.185" />

        <TextView
            android:id="@+id/Name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Name"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/Background5"
            app:layout_constraintEnd_toEndOf="@+id/Background3"
            app:layout_constraintHorizontal_bias="0.11"
            app:layout_constraintStart_toStartOf="@+id/Background3"
            app:layout_constraintTop_toTopOf="@+id/Background5" />

        <TextView
            android:id="@+id/Gender"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Gender"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background6"
            app:layout_constraintEnd_toEndOf="@+id/background4"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="@+id/background4"
            app:layout_constraintTop_toBottomOf="@+id/Background5" />

        <TextView
            android:id="@+id/Identity"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Identity"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background7"
            app:layout_constraintEnd_toEndOf="@+id/background4"
            app:layout_constraintHorizontal_bias="0.08"
            app:layout_constraintStart_toStartOf="@+id/background4"
            app:layout_constraintTop_toBottomOf="@+id/background6" />

        <TextView
            android:id="@+id/Email"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Email"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background9"
            app:layout_constraintEnd_toEndOf="@+id/background4"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="@+id/background4"
            app:layout_constraintTop_toBottomOf="@+id/background8"
            app:layout_constraintVertical_bias="0.52" />

        <TextView
            android:id="@+id/phone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Phone"
            android:textColor="#000000"
            android:textSize="16sp"
            app:layout_constraintBottom_toBottomOf="@+id/background8"
            app:layout_constraintEnd_toEndOf="@+id/background4"
            app:layout_constraintHorizontal_bias="0.09"
            app:layout_constraintStart_toStartOf="@+id/background4"
            app:layout_constraintTop_toBottomOf="@+id/background7" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="@+id/background4"
            app:layout_constraintEnd_toEndOf="@+id/background4"
            app:layout_constraintHorizontal_bias="0.83"
            app:layout_constraintStart_toEndOf="@+id/Nickname"
            app:layout_constraintTop_toBottomOf="@+id/Background3" />
    </androidx.constraintlayout.widget.ConstraintLayout>



</androidx.drawerlayout.widget.DrawerLayout>